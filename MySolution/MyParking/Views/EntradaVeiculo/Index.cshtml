@model IEnumerable<MyParking.DAL.Models.EntradaVeiculo>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Registrar nova entrada", "Create")
</p>

<table>

    
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id_Entrada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PlacaVeiculo)
        </th>

        <th>

        </th>
    </tr>
    
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id_Entrada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlacaVeiculo)
            </td>
        </tr>
    }

</table>

